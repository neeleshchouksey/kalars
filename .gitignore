#-------------------------
# Environment Files
#-------------------------
# These should never be under version control,
# as it poses a security risk.
.env

#-------------------------
# Temporary Files
#-------------------------


writable/cache/*
!writable/cache/index.html

writable/logs/*
!writable/logs/index.html

writable/session/*
!writable/session/index.html

writable/uploads/*
!writable/uploads/index.html

writable/debugbar/*
!writable/debugbar/.gitkeep

writable/**/*.db
writable/**/*.sqlite

php_errors.log

#-------------------------
# Composer
#-------------------------
vendor/
composer.lock

#-------------------------
# IDE / Development Files
#-------------------------

# Jetbrains editors (PHPStorm, etc)
.idea/
*.iml

app/Config/Database.php


klrdmin/.env

klrdmin/writable/cache/*
!klrdmin/writable/cache/index.html

klrdmin/writable/logs/*
!klrdmin/writable/logs/index.html

klrdmin/writable/session/*
!klrdmin/writable/session/index.html

klrdmin/writable/uploads/*
!klrdmin/writable/uploads/index.html

klrdmin/writable/debugbar/*
!klrdmin/writable/debugbar/.gitkeep

klrdmin/writable/**/*.db
klrdmin/writable/**/*.sqlite

klrdmin/php_errors.log

#-------------------------
# Composer
#-------------------------
klrdmin/vendor/
klrdmin/composer.lock

#-------------------------
# IDE / Development Files
#-------------------------

# Jetbrains editors (PHPStorm, etc)
klrdmin/.idea/
klrdmin/*.iml

klrdmin/app/Config/Database.php